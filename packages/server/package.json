{
  "name": "@foton-challenge/server",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" --ignore *.spec.js --out-dir dist --copy-files --source-maps --verbose",
    "clean": "rm -rf dist",
    "copy-to-package": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" scripts/copySchemaToPackage.js",
    "graphql": "webpack --watch --progress --config webpack.config.js",
    "start": "node dist/graphql/index.js",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "update-schema": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" scripts/updateSchema.js && yarn copy-to-package"
  },
  "dependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@entria/graphql-mongoose-loader": "^4.3.0",
    "@foton-challenge/babel": "0.0.1",
    "@koa/router": "^8.0.8",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.4",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "env-var": "^6.0.4",
    "graphql": "^14.6.0",
    "graphql-playground-middleware": "^1.1.2",
    "graphql-relay": "^0.6.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-express": "^1.1.0",
    "koa-graphql": "^0.8.0",
    "koa-logger": "^3.2.1",
    "koa-multer": "^1.0.2",
    "mongoose": "^5.9.3",
    "pretty-format": "^25.1.0",
    "subscriptions-transport-ws": "^0.9.16",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@types/dotenv-safe": "^8.1.0",
    "@types/graphql-relay": "^0.4.11",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-convert": "^1.2.3",
    "@types/koa-cors": "^0.0.0",
    "@types/koa-graphql": "^0.8.3",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-multer": "^1.0.0",
    "@types/koa__router": "^8.0.2",
    "@types/mongoose": "^5.7.3",
    "@types/node": "^13.9.0",
    "babel-loader": "^8.0.6",
    "mongodb-memory-server": "^6.3.2",
    "reload-server-webpack-plugin": "^1.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
